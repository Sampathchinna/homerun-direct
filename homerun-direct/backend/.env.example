# Enable SQL query logging
LOG_QUERY=True

# Email settings
DEFAULT_FROM_EMAIL=your_email@example.com
EMAIL_HOST_USER=your_email@example.com
EMAIL_HOST_PASSWORD=your_email_app_password

# Frontend URL for password resets, email links (used by backend)
FRONTEND_URL=http://localhost:3000/
CURRENT_DIRECT_LOGIN_URL=https://your-frontend-app.com/authentications/sessions

# Google OAuth
SOCIAL_AUTH_GOOGLE_CLIENT_ID=your_google_oauth_client_id

# Stripe API keys
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Django secret key
SECRET_KEY=your_django_secret_key

# Enable PostgreSQL
USE_POSTGRES=True

# PostgreSQL config
PG_DB_NAME=your_database_name
PG_USER=your_db_user
PG_PASSWORD=your_db_password
PG_HOST=localhost
PG_PORT=5432

# Redis (optional if you're using caching)
REDIS_PASSWORD=your_redis_password
